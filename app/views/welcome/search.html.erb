<table>
<tbody>
    <% @songs.each do |song| %>
      <div class='row'>
        <div class='col-sm-8'>
          <td><% song.title %></td>
          <td><% song.artist %></td>
          <td><% song.image %></td>
          <td><% song.id %></td>
          <br>
            <iframe width="100%" height="150" scrolling="no" frameborder="no" src=" https://w.soundcloud.com/player/?url=https://api.soundcloud.com/tracks/<%= song.id %>">
            </iframe>
         </div>
        </div>

        <a href="/repost/<%= song.id %>">Repost</a>
      </tr>
    <% end %>
  </tbody>
</table>